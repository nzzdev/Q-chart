{
  "$schema": "https://vega.github.io/schema/vega/v5.json",
  "signals": [
    {
      "name": "height",
      "update": "span(range('yScale'))"
    },
    {
      "name": "dotGroupHeight",
      "value": 24
    },
    {
      "name": "groupPadding",
      "value": 16
    },
    {
      "name": "groupHeight",
      "update": "dotGroupHeight + groupPadding"
    }
  ],
  "data": [
    {
      "name": "table",
      "values": []
    }
  ],
  "scales": [
    {
      "name": "xScale",
      "type": "linear",
      "range": "width",
      "domain": {
        "data": "table",
        "field": "yValue"
      },
      "nice": true,
      "zero": true,
      "round": true
    },
    {
      "name": "yScale",
      "type": "band",
      "range": {
        "step": {
          "signal": "groupHeight"
        }
      },
      "domain": {
        "data": "table",
        "field": "xValue"
      },
      "paddingInner": {
        "signal": "groupPadding / groupHeight"
      },
      "paddingOuter": 0,
      "round": true
    },
    {
      "name": "colorScale",
      "type": "ordinal",
      "range": {
        "scheme": "categorical_computed_normal"
      },
      "domain": {
        "data": "table",
        "field": "cValue"
      }
    },
    {
      "name": "colorScaleLight",
      "type": "ordinal",
      "range": {
        "scheme": "categorical_computed_light"
      },
      "domain": {
        "data": "table",
        "field": "cValue"
      }
    }
  ],
  "marks": [
    {
      "type": "group",
      "name": "blocks",
      "from": {
        "facet": {
          "name": "series",
          "data": "table",
          "groupby": "xValue"
        }
      },
      "encode": {
        "enter": {
          "y": {
            "scale": "yScale",
            "field": "xValue"
          },
          "x": {
            "value": 0
          }
        }
      },
      "marks": [
        {
          "type": "group",
          "encode": {
            "enter": {
              "y": {
                "value": 0
              },
              "x": {
                "value": 0
              },
              "width": {
                "signal": "width"
              },
              "height": {
                "signal": "dotGroupHeight"
              }
            }
          },
          "data": [
            {
              "name": "xValues",
              "source": "series",
              "transform": [
                {
                  "type": "aggregate",
                  "groupby": ["xValue"]
                }
              ]
            }
          ],
          "marks": [
            {
              "type": "symbol",
              "name": "circle",
              "from": {
                "data": "series"
              },
              "encode": {
                "enter": {
                  "y": {
                    "signal": "dotGroupHeight / 2 + (datum.posCorrectionFactor * 11)"
                  },
                  "x": {
                    "scale": "xScale",
                    "field": "yValue"
                  },
                  "stroke": {
                    "value": "white"
                  },
                  "strokeWidth": {
                    "value": 2
                  },
                  "size": {
                    "value": 121
                  },
                  "fill": [
                    {
                      "test": "datum.colorLight && datum.isHighlighted === false",
                      "field": "colorLight"
                    },
                    {
                      "test": "datum.color && datum.isHighlighted === true",
                      "field": "color"
                    },
                    {
                      "test": "datum.isHighlighted === true",
                      "scale": "colorScale",
                      "field": "cValue"
                    },
                    {
                      "test": "datum.isHighlighted === false",
                      "scale": "colorScaleLight",
                      "field": "cValue"
                    },
                    {
                      "test": "datum.color",
                      "field": "color"
                    },
                    {
                      "scale": "colorScale",
                      "field": "cValue"
                    }
                  ]
                }
              }
            }
          ]
        }
      ]
    }
  ],
  "axes": [
    {
      "orient": "top",
      "scale": "xScale",
      "domain": false,
      "grid": true,
      "labels": true,
      "ticks": true,
      "tickCount": 6,
      "labelSeparation": 4,
      "labelOverlap": "parity"
    },
    {
      "orient": "left",
      "scale": "yScale",
      "domain": false,
      "grid": true,
      "labels": true,
      "ticks": false,
      "labelPadding": 8,
      "encode": {
        "title": {
          "update": {
            "align": {
              "value": "right"
            },
            "angle": {
              "value": 0
            },
            "baseline": {
              "value": "bottom"
            },
            "x": {
              "value": 0
            },
            "y": {
              "value": -30
            }
          }
        }
      }
    }
  ],
  "autosize": {
    "type": "fit-x",
    "contains": "padding"
  }
}
