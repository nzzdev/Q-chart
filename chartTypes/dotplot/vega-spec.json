{
  "signals": [
    {
      "name": "height",
      "update": "span(range('yScale'))"
    },
    {
      "name": "numberOfDataSeries",
      "value": 1
    },
    {
      "name": "labelHeight",
      "value": 0
    },
    {
      "name": "dotGroupHeight",
      "value": 24
    },
    {
      "name": "groupPadding",
      "value": 16
    },
    {
      "name": "groupHeight",
      "update": "dotGroupHeight + labelHeight + groupPadding"
    }
  ],
  "data": [
    {
      "name": "table",
      "values": []
    }
  ],
  "scales": [
    {
      "name": "xScale",
      "type": "linear",
      "range": "width",
      "domain": {
        "data": "table",
        "field": "yValue"
      },
      "nice": true,
      "zero": true,
      "round": true
    },
    {
      "name": "yScale",
      "type": "band",
      "range": {
        "step": {
          "signal": "groupHeight"
        }
      },
      "domain": {
        "data": "table",
        "field": "xValue"
      },
      "paddingInner": {
        "signal": "groupPadding / groupHeight"
      },
      "paddingOuter": 0,
      "round": true
    },
    {
      "name": "colorScale",
      "type": "ordinal",
      "range": "category",
      "domain": {
        "data": "table",
        "field": "cValue"
      }
    }
  ],
  "marks": [
    {
      "type": "group",
      "name": "blocks",
      "from": {
        "facet": {
          "name": "series",
          "data": "table",
          "groupby": "xValue"
        }
      },
      "encode": {
        "enter": {
          "y": {
            "scale": "yScale",
            "field": "xValue"
          },
          "x": {
            "value": 0
          },
          "height": {
            "signal": "bandwidth('yScale')"
          }
        }
      },
      "signals": [
        {
          "name": "height",
          "update": "bandwidth('yScale') - labelHeight"
        }
      ],
      "marks": [
        {
          "type": "group",
          "encode": {
            "enter": {
              "y": {
                "signal": "labelHeight"
              },
              "x": {
                "value": 0
              },
              "width": {
                "signal": "width"
              },
              "height": {
                "signal": "height"
              }
            }
          },
          "marks": [
            {
              "type": "symbol",
              "name": "circle",
              "from": {
                "data": "series"
              },
              "encode": {
                "enter": {
                  "y": {
                    "signal": "dotGroupHeight / 2 + (datum.posCorrectionFactor * 10)"
                  },
                  "x": {
                    "scale": "xScale",
                    "field": "yValue"
                  },
                  "fill": {
                    "scale": "colorScale",
                    "field": "cValue"
                  },
                  "stroke": {
                    "value": "white"
                  },
                  "strokeWidth": {
                    "value": 2
                  },
                  "size": {
                    "value": 100
                  }
                }
              }
            }
          ]
        }
      ]
    }
  ],
  "axes": [
    {
      "orient": "top",
      "scale": "xScale",
      "domain": false,
      "grid": true,
      "ticks": true,
      "tickCount": 6
    },
    {
      "orient": "left",
      "scale": "yScale",
      "domain": false,
      "grid": true,
      "labels": true,
      "ticks": false,
      "encode": {
        "title": {
          "update": {
            "align": {
              "value": "right"
            },
            "angle": {
              "value": 0
            },
            "baseline": {
              "value": "bottom"
            },
            "x": {
              "value": 0
            },
            "y": {
              "value": -30
            }
          }
        }
      }
    }
  ],
  "autosize": {
    "type": "fit-x",
    "contains": "padding"
  }
}