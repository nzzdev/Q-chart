{
  "$schema": "https://vega.github.io/schema/vega/v3.0.json",
  "autosize": {
    "type": "fit"
  },
  "height": 300,
  "scales": [
    {
      "name": "xScale",
      "type": "point",
      "range": "width",
      "domain": {
        "data": "table",
        "field": "xValue"
      }
    },
    {
      "name": "yScale",
      "type": "linear",
      "range": "height",
      "nice": true,
      "zero": true,
      "domain": {
        "data": "table",
        "field": "yValue"
      }
    },
    {
      "name": "color",
      "type": "ordinal",
      "range": "category",
      "domain": {
        "data": "table",
        "field": "cValue"
      }
    }
  ],
  "axes": [
    {
      "orient": "bottom",
      "scale": "xScale",
      "grid": true,
      "labelFlush": true,
      "labelFlushOffset": false
    },
    {
      "orient": "left",
      "scale": "yScale",
      "grid": true
    }
  ],
  "marks": [
    {
      "type": "group",
      "from": {
        "facet": {
          "name": "series",
          "data": "table",
          "groupby": "cValue"
        }
      },
      "marks": [
        {
          "type": "line",
          "from": {
            "data": "series"
          },
          "encode": {
            "enter": {
              "x": {
                "scale": "xScale",
                "field": "xValue"
              },
              "y": {
                "scale": "yScale",
                "field": "yValue"
              },
              "stroke": {
                "scale": "color",
                "field": "cValue"
              },
              "strokeWidth": {
                "value": 2
              }
            }
          }
        }
      ]
    }
  ],
  "config": {
    "axis": {
      "labels": true,
      "labelAngle": 0
    }
  }
}
