{
  "$schema": "https://vega.github.io/schema/vega/v5.json",
  "autosize": {
    "type": "fit"
  },
  "height": 220,
  "scales": [
    {
      "name": "xScale",
      "type": "point",
      "range": "width",
      "domain": {
        "data": "table",
        "field": "xValue"
      }
    },
    {
      "name": "yScale",
      "type": "linear",
      "range": "height",
      "nice": 4,
      "zero": true,
      "domain": {
        "data": "table",
        "field": "yValue"
      }
    },
    {
      "name": "colorScale",
      "type": "ordinal",
      "range": {
        "scheme": "categorical_computed_normal"
      },
      "domain": {
        "data": "table",
        "field": "cValue",
        "sort": true
      }
    },
    {
      "name": "colorScaleLight",
      "type": "ordinal",
      "range": {
        "scheme": "categorical_computed_light"
      },
      "domain": {
        "data": "table",
        "field": "cValue",
        "sort": true
      }
    }
  ],
  "axes": [
    {
      "orient": "bottom",
      "scale": "xScale",
      "domain": false,
      "grid": false,
      "labels": true,
      "labelFlush": true,
      "labelBound": true,
      "labelFlushOffset": 0,
      "labelOverlap": "parity",
      "labelSeparation": 4,
      "encode": {
        "title": {
          "update": {
            "align": {
              "value": "right"
            },
            "baseline": {
              "value": "top"
            },
            "x": {
              "signal": "width"
            },
            "y": {
              "value": 30
            }
          }
        }
      }
    },
    {
      "orient": "left",
      "scale": "yScale",
      "domain": false,
      "grid": true,
      "ticks": false,
      "labels": true,
      "tickCount": 4,
      "labelPadding": 2
    }
  ],
  "marks": [
    {
      "type": "group",
      "name": "data-series",
      "from": {
        "facet": {
          "name": "series",
          "data": "table",
          "groupby": "cValue"
        }
      },
      "marks": [
        {
          "type": "line",
          "from": {
            "data": "series"
          },
          "encode": {
            "enter": {
              "x": {
                "scale": "xScale",
                "field": "xValue"
              },
              "y": {
                "scale": "yScale",
                "field": "yValue"
              },
              "strokeWidth": {
                "value": 2
              },
              "defined": {
                "signal": "datum.yValue !== null"
              },
              "strokeJoin": {
                "value": "round"
              },
              "stroke": [
                {
                  "test": "datum.isHighlighted === true",
                  "scale": "colorScale",
                  "field": "cValue"
                },
                {
                  "test": "datum.isHighlighted === false",
                  "scale": "colorScaleLight",
                  "field": "cValue"
                },
                {
                  "test": "datum.color",
                  "field": "color"
                },
                {
                  "scale": "colorScale",
                  "field": "cValue"
                }
              ]
            }
          }
        },
        {
          "name": "annotations-events-line",
          "type": "rule",
          "from": { "data": "annotations-events" },
          "encode": {
            "enter": {
              "x": { "signal": "floor(scale('xScale', datum.xValue)) + 0.5" },
              "y": { "value": -10 },
              "y2": { "field": { "group": "height", "level": 2 } },
              "stroke": { "value": "#fff" },
              "strokeOpacity": { "value": 0.5 },
              "strokeWidth": { "value": 3 }
            }
          }
        },
        {
          "type": "rule",
          "from": { "data": "annotations-events-line" },
          "encode": {
            "enter": {
              "x": { "field": "x" },
              "y": { "field": "y" },
              "y2": { "field": "y2" },
              "stroke": { "value": "#6e6e7e" },
              "strokeDash": { "value": [1, 1] },
              "strokeWidth": { "value": 1 }
            }
          }
        },
        {
          "type": "symbol",
          "from": { "data": "annotations-events-line" },
          "encode": {
            "enter": {
              "x": { "field": "x" },
              "y": { "value": -18 },
              "size": { "signal": "16 * 16" },
              "stroke": { "value": "#6e6e7e" },
              "strokeWidth": { "value": 1 }
            }
          }
        },
        {
          "type": "text",
          "from": { "data": "annotations-events-line" },
          "encode": {
            "enter": {
              "x": { "field": "x" },
              "y": { "value": -17 },
              "align": { "value": "center" },
              "baseline": { "value": "middle" },
              "fill": { "value": "#6e6e7e" },
              "fontWeight": { "value": "bold" },
              "text": { "signal": "datum.datum.xIndex + 1" }
            }
          }
        }
      ]
    }
  ],
  "config": {
    "axis": {
      "labelAngle": 0
    }
  }
}
